{
  "app": {
    "name": "config-manager",
    "version": "${HOPEIT_APPS_API_VERSION}"
  },
  "engine": {
    "import_modules": [
      "hopeit.config_manager"
    ]
  },
  "settings": {
    "client": {
      "timeout": 10
    }
  },
  "events": {
    "runtime_apps_config": {
      "type": "GET",
      "plug_mode": "Standalone",
      "setting_keys": [
        "client"
      ],
      "auth": [
        "Unsecured"
      ]
    },
    "cluster_apps_config": {
      "type": "GET",
      "plug_mode": "Standalone",
      "setting_keys": [
        "client"
      ],
      "auth": [
        "Unsecured"
      ]
    }
  }
}