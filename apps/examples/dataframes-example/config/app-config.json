{
    "app": {
        "name": "dataframes-example",
        "version": "${HOPEIT_APPS_API_VERSION}"
    },
    "plugins": [
        {
            "name": "dataframes",
            "version": "${HOPEIT_APPS_API_VERSION}"
        }
    ],
    "engine": {
        "track_headers": [
        ],
        "cors_origin": "*"
    },
    "settings": {
        "data_storage": {
            "ingest_data_path": "apps/examples/dataframes-example/data/raw",
            "data_storage_path": "apps/examples/dataframes-example/data/storage"
        },
        "model_storage": {
            "model_storage_path": "apps/examples/dataframes-example/data/models"
        }
    },
    "events": {
        "training_pipeline": {
            "type": "GET",
            "setting_keys": ["data_storage", "model_storage"]
        },
        "predict": {
            "type": "POST",
            "setting_keys": ["model_storage"]
        }
    }
}
