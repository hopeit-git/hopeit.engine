{
  "app": {
    "name": "simple-benchmark",
    "version": "${HOPEIT_APPS_API_VERSION}"
  },
  "engine" : {
    "import_modules": ["simple_benchmark"]
  },
  "env" : {
    "fs": {
      "data_path": "/tmp/{auto}/"
    },
    "redis": {
      "address": "redis://localhost:6379"
    }
  },
  "events": {
    "give_me_something": {
      "type": "GET"
    },
    "query_something_fs": {
      "type": "GET"
    },
    "save_something_fs" : {
      "type": "POST"
    },
    "query_something_redis": {
      "type": "GET"
    },
    "save_something_redis" : {
      "type": "POST"
    }
  }
}
