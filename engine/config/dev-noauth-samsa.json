{
  "streams": {
    "stream_manager": "hopeit.samsa.streams.SamsaStreamManager",
    "connection_str": "api_version=${HOPEIT_APPS_API_VERSION};push_nodes=${SAMSA_PUSH_NODES};consume_nodes=${SAMSA_CONSUME_NODES}",
    "delay_auto_start_seconds": 10
  },
  "logging": {
    "log_level": "DEBUG",
    "log_path": "work/logs/apps/"
  },
  "auth": {
    "auth_passphrase": "",
    "secrets_location": "/tmp",
    "enabled": false,
    "create_keys": false,
    "default_auth_methods": ["Unsecured"]
  },
  "api": {
    "docs_path": "/api/docs"
  }
}
